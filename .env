# .env for TWS Local Project

# MongoDB Connection String (REQUIRED for persistent data)
# Replace 'your_username', 'your_password', 'your_cluster_url' with your actual MongoDB Atlas credentials.
MONGODB_URI=mongodb+srv://your_username:your_password@your_cluster_url/tws?retryWrites=true&w=majority

# Port for the backend server
PORT=3001

